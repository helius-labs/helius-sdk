{
  "name": "helius-sdk",
  "version": "1.4.2",
  "description": "SDK for the Helius API (https://helius.xyz)",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "tsc",
